{
  "name": "victor-opvang-tracker",
  "version": 2,
  "functions": {
    "pages/api/log-pickup.js": {
      "maxDuration": 10
    },
    "pages/api/get-logs.js": {
      "maxDuration": 10
    },
    "pages/api/export-csv.js": {
      "maxDuration": 15
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
